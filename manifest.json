{
  "name": "Duplicate Favorites Remover",
  "author": "Derviş Öksüzoğlu",
  "version": "1.1.0",
  "incognito": "not_allowed",
  "manifest_version": 3,
  "description": "An extension to identify and remove duplicate favorites (a.k.a bookmarks) in Microsoft Edge web browser. Source code is available at: https://gokacinlar.github.io/Edge-Duplicate-Favorites-Remover-Extension/",
  "icons": {
    "16": "icons/icon-16x16.png",
    "32": "icons/icon-32x32.png",
    "48": "icons/icon-48x48.png",
    "128": "icons/icon-128x128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "/assets/css/custom.css"
      ],
      "js": [
        "/src/app.js"
      ]
    }
  ],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Duplicate Favorites Remover",
    "default_icon": {
      "16": "icons/icon-16x16.png",
      "48": "icons/icon-48x48.png"
    }
  },
  "permissions": [
    "bookmarks"
  ],
  "default_locale": "en",
  "web_accessible_resources": [
    {
      "resources": [
        "/images/logo.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}